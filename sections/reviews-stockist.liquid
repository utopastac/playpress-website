{{ 'reviews.css' | asset_url | stylesheet_tag }}



<section is="section" class="reviews-container">
  <h5>Stockist reviews</h5>

  <div is="reviews">
    <div>
      {% for review in metaobjects.stockist_review.values %}
        {% render 'review', review: review %}
      {% endfor %}
    </div>
  </div>
</section>

{% schema %}
{
  "name": "Reviews Stockist",
  "presets": [
    {
      "name": "Reviews Stockist",
    }
  ]
}
{% endschema %}
